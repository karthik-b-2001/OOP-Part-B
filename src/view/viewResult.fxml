<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.viewResultController">
    <children>
        <TableView fx:id="resultsTableView" layoutX="75.0" layoutY="60.0" prefHeight="281.0" prefWidth="451.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="49.0" text="ID" />
                <TableColumn fx:id="nameColumn" prefWidth="100.0" text="Name" />
                <TableColumn fx:id="resultColumn" prefWidth="87.0" text="Result" />
                <TableColumn fx:id="statusColumn" prefWidth="89.0" text="Status" />
                <TableColumn fx:id="courseColumn" prefWidth="123.0" text="Course" />
            </columns>
        </TableView>
        <Button layoutX="273.0" layoutY="363.0" mnemonicParsing="false" onAction="#showResult" stylesheets="@../CSS/button.css" text="Update" />
        <Label layoutX="233.0" layoutY="15.0" prefHeight="25.0" prefWidth="191.0" text="View Results">
            <font>
                <Font name="Comic Sans MS" size="23.0" />
            </font>
        </Label>
        <TextField fx:id="prompt" layoutX="30.0" layoutY="31.0" style="-fx-background-color: white;" />
      <Button layoutX="30.0" layoutY="362.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="55.0" stylesheets="@../CSS/button.css" text="Back" />
    </children>
</AnchorPane>
